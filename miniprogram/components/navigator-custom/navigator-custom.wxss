.big-box {
  background-color: #2c5cff;
}

.navigator {
  display: flex;
  justify-content: center;
  width: 750rpx;
  position: absolute;
  height: 150rpx;
  z-index: 1
}

.nav-image {
  height: 25px;
  width: 25px;
}

.navigator text {
  color: white;
}

.homepage {
  height: 30px;
  width: 50px;
  left: 0;
  display: flex;
  flex-wrap: unwrap;
  margin-left: -650rpx;
}

.homepage image {
  height: 25px;
  width: 25px;
}

.nav-title {
  display: flex;
  flex-wrap: unwrap;
  margin-left: -25rpx;
  position: absolute;
  font-size: 20px;
}

.backNavigator{
  border: 1px solid black;
  border-radius: 16px;
  width: 60px;
  display: flex;
  flex-wrap: unwrap;
}
.backhome{
  border-left: 1px solid #777;
}