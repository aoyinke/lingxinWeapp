.hole_list {
  display: flex;
  flex-wrap: wrap;
}

.user {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 30%;
  margin: 10rpx;
  background-color: #32cd32;
  box-sizing: border-box;
  box-shadow: 2px 2px 10px #bbb;
  text-shadow: 5px 5px 5px 	#696969;

}

.holeImg {
  width: 150rpx;
  height: 150rpx;
  margin-top: 20rpx;
}

.holeImg image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
